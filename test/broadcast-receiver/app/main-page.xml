<Page xmlns="http://www.nativescript.org/tns.xsd" navigatingTo="onNavigatingTo" >

    <StackLayout >
        <Button text="navigate" tap="navigate" />
        <Label text="{{ info }}" textWrap="true" fontSize="14"></Label>
        <Label text="{{ batteryLife + ' percent' }}" textWrap="true" fontSize="24"></Label>
        <Button text="startReceiver" tap="startReceiver" />
        <Button text="stopReceiver" tap="stopReceiver" />
    </StackLayout>

</Page>